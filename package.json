{
  "name": "muzheal",
  "version": "1.0.0",
  "description": "",
  "main": "./src/app.js",
  "engines": {
    "node": "16.x"
  },
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src/**/*.js",
    "build": "babel src -d dist --copy-files",
    "deploy": "babel src -d build --copy-files",
    "zip": "npm-build-zip",
    "build:prod": "npm run lint && npm run deploy && npm run zip",
    "build:dev": "npm run lint && npm run build && npm run zip:dev",
    "build:testing": "npm run lint && npm run build && npm run zip:testing",
    "start": "node -r dotenv/config dist/app.js dotenv_config_path=.environment/prod.env",
    "start:dev": "node -r dotenv/config dist/app.js dotenv_config_path=.environment/dev.env",
    "start:testing": "node -r dotenv/config dist/app.js dotenv_config_path=.environment/testing.env",
    "start:prod": "npm run lint && npm run build && node -r dotenv/config dist/app.js dotenv_config_path=.environment/prod.env",
    "start:ldev": "./node_modules/.bin/nodemon -e js,env -w .environment --watch src/ --exec \"npm run lint && npm run build && node -r dotenv/config\" dist/app.js dotenv_config_path=.environment/dev.env",
    "start:local_dummy": "./node_modules/.bin/nodemon -e js,env -w .environment --watch src/ --exec \"npm run lint && npm run build && node -r dotenv/config\" dist/app.js dotenv_config_path=.environment/local.env",
    "start:local": "nodemon -r dotenv/config src/app.js dotenv_config_path=.environment/local.env"
  },
  "author": "Madhav Technology",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "aws-sdk": "^2.1251.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "eslint": "^8.24.0",
    "express": "^4.18.1",
    "express-http-context": "^1.2.4",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.3.0",
    "google-auth-library": "^8.5.2",
    "http-status": "^1.5.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^6.6.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.20",
    "passport": "^0.6.0",
    "path": "^0.12.7",
    "puppeteer": "^19.4.1",
    "shortid": "^2.2.16",
    "stripe": "^11.13.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/node": "^7.19.1"
  }
}
